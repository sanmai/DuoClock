#!/usr/bin/env php
<?php

declare(strict_types=1);

foreach ([__DIR__ . '/../../../autoload.php', __DIR__ . '/../vendor/autoload.php'] as $file) {
    if (file_exists($file)) {
        require $file;
        break;
    }
}

use Introspect\Application;

$application = new Application();
$application->run();